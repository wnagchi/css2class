{
  "name": "class2css",
  "version": "1.0.0",
  "description": "Dynamic CSS Generator for WeChat Mini Program",
  "main": "src/index.js",
  "bin": {
    "class2css": "./bin/class2css.js"
  },
  "scripts": {
    "start": "node bin/class2css.js",
    "start:no-watch": "node bin/class2css.js --no-watch",
    "dev": "node bin/class2css.js --config ./class2css.config.js",
    "test": "node src/example.js",
    "help": "node bin/class2css.js --help",
    "version": "node bin/class2css.js --version"
  },
  "keywords": [
    "css",
    "wechat",
    "miniprogram",
    "tailwind",
    "utility",
    "generator"
  ],
  "author": "CMViking",
  "license": "MIT",
  "dependencies": {
    "chokidar": "^3.5.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cmviking/class2css.git"
  },
  "bugs": {
    "url": "https://github.com/cmviking/class2css/issues"
  },
  "homepage": "https://github.com/cmviking/class2css#readme"
} 